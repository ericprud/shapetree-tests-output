@prefix ldp: <http://www.w3.org/ns/ldp#>.
@prefix foot: <http://www.w3.org/ns/blueprints#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix dcterms: <http://purl.org/dc/terms/>.

<> a ldp:BasicContainer;
    dcterms:title "MultiCalApp Directory";
    foot:installedIn _:b48, _:b50.
_:b48 foot:app <//store.example/MultiCalApp>;
    foot:blueprintRoot </cal/CalendarBlueprint#calendar>;
    foot:blueprintInstancePath "Shared/Calendar/".
<//store.example/MultiCalApp> foot:name "MultiCalApp".
_:b50 foot:app <//store.example/MultiCalApp>;
    foot:blueprintRoot </cal/GoogleBlueprint#top>;
    foot:blueprintInstancePath "Shared/Google/".
