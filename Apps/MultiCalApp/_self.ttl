@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix ldp: <http://www.w3.org/ns/ldp#>.
@prefix tree: <http://www.w3.org/ns/shapetree#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

<> a ldp:BasicContainer;
    dcterms:title "MultiCalApp Directory";
    tree:installedIn _:b40, _:b42.
_:b40 tree:app <http://store.example/MultiCalApp>;
    tree:shapeTreeRoot </cal/CalendarShapeTree#calendar>;
    tree:shapeTreeInstancePath "Data/Calendar/".
<http://store.example/MultiCalApp> tree:name "MultiCalApp".
_:b42 tree:app <http://store.example/MultiCalApp>;
    tree:shapeTreeRoot </cal/GoogleShapeTree#top>;
    tree:shapeTreeInstancePath "Data/Google/".
