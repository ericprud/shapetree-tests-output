@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix ldp: <http://www.w3.org/ns/ldp#>.
@prefix tree: <http://www.w3.org/ns/shapetree#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

<> a ldp:BasicContainer;
    dcterms:title "GhApp Directory";
    tree:installedIn _:b50, _:b54.
_:b50 tree:app <http://store.example/gh>;
    tree:shapeTreeRoot <http://localhost:12345/gh/ghShapeTree#root>;
    tree:shapeTreeInstancePath </Data/Git/>.
<http://store.example/gh> tree:name "GhApp".
_:b54 tree:app <http://store.example/gh>;
    tree:shapeTreeRoot <http://localhost:12345/gh/ghShapeTree#root>;
    tree:shapeTreeInstancePath </some/deep/path/Git/>.
